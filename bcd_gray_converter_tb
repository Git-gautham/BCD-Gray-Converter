#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ca6f74effe0 .scope module, "tb_bcd_gray_converter" "tb_bcd_gray_converter" 2 2;
 .timescale 0 0;
v0x5ca6f7506c50_0 .var "bcd", 3 0;
v0x5ca6f7506d30_0 .var "gray", 3 0;
v0x5ca6f7506e00_0 .var "mode", 0 0;
v0x5ca6f7506f00_0 .net "out", 3 0, v0x5ca6f7506ac0_0;  1 drivers
S_0x5ca6f74f0170 .scope module, "uut" "bcd_gray_converter" 2 9, 3 2 0, S_0x5ca6f74effe0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "bcd";
    .port_info 1 /INPUT 4 "gray";
    .port_info 2 /INPUT 1 "mode";
    .port_info 3 /OUTPUT 4 "out";
v0x5ca6f74b57b0_0 .net "bcd", 3 0, v0x5ca6f7506c50_0;  1 drivers
v0x5ca6f74b5ba0_0 .net "gray", 3 0, v0x5ca6f7506d30_0;  1 drivers
v0x5ca6f75069f0_0 .net "mode", 0 0, v0x5ca6f7506e00_0;  1 drivers
v0x5ca6f7506ac0_0 .var "out", 3 0;
E_0x5ca6f74b4870 .event edge, v0x5ca6f75069f0_0, v0x5ca6f74b57b0_0, v0x5ca6f74b5ba0_0, v0x5ca6f7506ac0_0;
    .scope S_0x5ca6f74f0170;
T_0 ;
    %wait E_0x5ca6f74b4870;
    %load/vec4 v0x5ca6f75069f0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5ca6f74b57b0_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ca6f74b5ba0_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f74b5ba0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5ca6f74b5ba0_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f7506ac0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5ca6f74b5ba0_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
    %load/vec4 v0x5ca6f7506ac0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5ca6f74b5ba0_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ca6f7506ac0_0, 4, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5ca6f74effe0;
T_1 ;
    %vpi_call 2 18 "$monitor", "Mode: %b, BCD Input: %b, Gray Input: %b, Output: %b", v0x5ca6f7506e00_0, v0x5ca6f7506c50_0, v0x5ca6f7506d30_0, v0x5ca6f7506f00_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ca6f7506e00_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x5ca6f7506c50_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ca6f7506e00_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x5ca6f7506d30_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bcd_gray_converter_tb.v";
    "bcd_gray_converter.v";
